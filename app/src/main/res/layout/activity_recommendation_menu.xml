<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background14"
    android:orientation="vertical"
    tools:context=".RecommendationMenu"
    tools:layout_editor_absoluteY="81dp">

    <android.support.v7.widget.Toolbar
        android:id="@id/my_toolbar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:background="#00ffffff"
        android:minHeight="?attr/actionBarSize"
        android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:popupTheme="@style/ThemeOverlay.AppCompat.Light"/>

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:text="서울의 야경명소 열세곳"
        android:textColor="#ffffff"
        android:textSize="30dp"
        android:fontFamily="@font/seoulhangang"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="56dp" />

    <android.support.constraint.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="match_parent"
        android:layout_height="300dp"
        android:layout_marginBottom="88dp"
        android:layout_marginTop="140dp"
        android:background="@drawable/map01"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">


        <ImageButton
            android:id="@+id/banpo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="12dp"
            android:background="@drawable/bluestar"
            app:layout_constraintBottom_toBottomOf="@+id/dungdung"
            app:layout_constraintEnd_toEndOf="@+id/dungdung"
            app:layout_constraintStart_toEndOf="@+id/dungdung"
            app:srcCompat="@drawable/bluestar" />

        <ImageButton
            android:id="@+id/sukchon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="86dp"
            android:layout_marginRight="86dp"
            android:background="@drawable/bluestar"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/dungdung"
            app:layout_constraintTop_toBottomOf="@+id/namsan"
            app:srcCompat="@drawable/bluestar" />

        <ImageButton
            android:id="@+id/noeul"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:layout_marginEnd="9dp"
            android:layout_marginRight="9dp"
            android:background="@drawable/bluestar"
            app:layout_constraintBottom_toTopOf="@+id/building63"
            app:layout_constraintEnd_toStartOf="@+id/building63"
            app:srcCompat="@drawable/bluestar" />

        <ImageButton
            android:id="@+id/olympic"
            android:layout_width="0dp"
            android:layout_height="37dp"
            android:layout_marginBottom="12dp"
            android:layout_marginEnd="65dp"
            android:layout_marginRight="65dp"
            android:layout_marginTop="23dp"
            android:background="@drawable/bluestar"
            app:layout_constraintBottom_toBottomOf="@+id/sukchon"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/ungbong"
            app:layout_constraintTop_toTopOf="@+id/ungbong"
            app:srcCompat="@drawable/bluestar" />

        <ImageButton
            android:id="@+id/naksan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="22dp"
            android:background="@drawable/bluestar"
            app:layout_constraintStart_toEndOf="@+id/kyeongbok"
            app:layout_constraintTop_toTopOf="@+id/bukak"
            app:srcCompat="@drawable/bluestar" />

        <ImageButton
            android:id="@+id/ungbong"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="18dp"
            android:layout_marginRight="18dp"
            android:background="@drawable/bluestar"
            app:layout_constraintBottom_toTopOf="@+id/sukchon"
            app:layout_constraintEnd_toStartOf="@+id/olympic"
            app:layout_constraintStart_toEndOf="@+id/namsan"
            app:srcCompat="@drawable/bluestar" />

        <ImageButton
            android:id="@+id/bukak"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="11dp"
            android:layout_marginStart="11dp"
            android:layout_marginTop="81dp"
            android:background="@drawable/bluestar"
            app:layout_constraintStart_toStartOf="@+id/kyeongbok"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/bluestar" />

        <ImageButton
            android:id="@+id/namsan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="10dp"
            android:layout_marginLeft="212dp"
            android:layout_marginRight="10dp"
            android:layout_marginStart="212dp"
            android:background="@drawable/bluestar"
            app:layout_constraintEnd_toStartOf="@+id/ungbong"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/seoullo"
            app:srcCompat="@drawable/bluestar" />

        <ImageButton
            android:id="@+id/dungdung"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginBottom="1dp"
            android:layout_marginEnd="49dp"
            android:layout_marginLeft="200dp"
            android:layout_marginRight="49dp"
            android:layout_marginStart="200dp"
            android:background="@drawable/bluestar"
            app:layout_constraintBottom_toBottomOf="@+id/sukchon"
            app:layout_constraintEnd_toStartOf="@+id/sukchon"
            app:layout_constraintStart_toEndOf="@+id/sunyu"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/seoullo"
            app:srcCompat="@drawable/bluestar" />

        <ImageButton
            android:id="@+id/kyeongbok"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/bluestar"
            app:layout_constraintStart_toStartOf="@+id/seoullo"
            app:layout_constraintTop_toBottomOf="@+id/bukak"
            app:srcCompat="@drawable/bluestar" />

        <ImageButton
            android:id="@+id/seoullo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="10dp"
            android:layout_marginRight="10dp"
            android:background="@drawable/bluestar"
            app:layout_constraintEnd_toStartOf="@+id/banpo"
            app:layout_constraintTop_toBottomOf="@+id/kyeongbok"
            app:srcCompat="@drawable/bluestar" />

        <ImageButton
            android:id="@+id/sunyu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="35dp"
            android:layout_marginLeft="129dp"
            android:layout_marginRight="35dp"
            android:layout_marginStart="129dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/bluestar"
            app:layout_constraintEnd_toStartOf="@+id/dungdung"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/seoullo"
            app:srcCompat="@drawable/bluestar" />

        <ImageButton
            android:id="@+id/building63"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="12dp"
            android:layout_marginEnd="26dp"
            android:layout_marginLeft="22dp"
            android:layout_marginRight="26dp"
            android:layout_marginStart="22dp"
            android:background="@drawable/bluestar"
            app:layout_constraintBottom_toBottomOf="@+id/dungdung"
            app:layout_constraintEnd_toEndOf="@+id/seoullo"
            app:layout_constraintStart_toStartOf="@+id/sunyu"
            app:srcCompat="@drawable/bluestar" />

    </android.support.constraint.ConstraintLayout>

</LinearLayout>